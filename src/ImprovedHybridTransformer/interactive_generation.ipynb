    "# CLAUDE_ADDED\n",
    "# 既存データとの比較分析\n",
    "def compare_with_original(generated_trajectories, condition_vector, trajectories, conditions):\n",
    "    \"\"\"\n",
    "    生成された軌道と類似条件の既存データを比較\n",
    "    \"\"\"\n",
    "    # 類似条件を見つける\n",
    "    condition_distances = np.linalg.norm(conditions - condition_vector, axis=1)\n",
    "    similar_indices = np.argsort(condition_distances)[:5]  # 最も類似する5つ\n",
    "    \n",
    "    plt.figure(figsize=(15, 10))\n",
    "    \n",
    "    # 軌道比較\n",
    "    plt.subplot(2, 3, 1)\n",
    "    # 既存の類似軌道\n",
    "    for i, idx in enumerate(similar_indices):\n",
    "        plt.plot(trajectories[idx, :, 0], trajectories[idx, :, 1], \n",
    "                'k-', alpha=0.6, linewidth=1, label='Similar Original' if i == 0 else '')\n",
    "    \n",
    "    # 生成された軌道\n",
    "    for i, traj in enumerate(generated_trajectories):\n",
    "        plt.plot(traj[:, 0], traj[:, 1], '--', linewidth=2, alpha=0.8, \n",
    "                label='Generated' if i == 0 else '')\n",
    "    \n",
    "    plt.xlabel('X Position')\n",
    "    plt.ylabel('Y Position')\n",
    "    plt.title('軌道比較: 生成 vs 類似既存データ')\n",
    "    plt.legend()\n",
    "    plt.grid(True, alpha=0.3)\n",
    "    plt.axis('equal')\n",
    "    \n",
    "    # 統計比較\n",
    "    similar_trajectories = trajectories[similar_indices]\n",
    "    \n",
    "    # 軌道長比較\n",
    "    plt.subplot(2, 3, 2)\n",
    "    original_lengths = []\n",
    "    for traj in similar_trajectories:\n",
    "        length = np.sum(np.sqrt(np.sum(np.diff(traj, axis=0)**2, axis=1)))\n",
    "        original_lengths.append(length)\n",
    "    \n",
    "    generated_lengths = []\n",
    "    for traj in generated_trajectories:\n",
    "        length = np.sum(np.sqrt(np.sum(np.diff(traj, axis=0)**2, axis=1)))\n",
    "        generated_lengths.append(length)\n",
    "    \n",
    "    plt.boxplot([original_lengths, generated_lengths], labels=['Original', 'Generated'])\n",
    "    plt.ylabel('軌道長')\n",
    "    plt.title('軌道長の比較')\n",
    "    plt.grid(True, alpha=0.3)\n",
    "    \n",
    "    # 終点誤差比較\n",
    "    plt.subplot(2, 3, 3)\n",
    "    original_endpoints = similar_trajectories[:, -1, :]\n",
    "    generated_endpoints = generated_trajectories[:, -1, :]\n",
    "    \n",
    "    plt.scatter(original_endpoints[:, 0], original_endpoints[:, 1], \n",
    "               alpha=0.7, label='Original', s=100, color='black')\n",
    "    plt.scatter(generated_endpoints[:, 0], generated_endpoints[:, 1], \n",
    "               alpha=0.7, label='Generated', s=100, color='red')\n",
    "    plt.xlabel('終点X座標')\n",
    "    plt.ylabel('終点Y座標')\n",
    "    plt.title('終点位置の比較')\n",
    "    plt.legend()\n",
    "    plt.grid(True, alpha=0.3)\n",
    "    plt.axis('equal')\n",
    "    \n",
    "    # 条件パラメータ表示\n",
    "    plt.subplot(2, 3, 4)\n",
    "    condition_names = ['動作時間', '終点誤差', 'ジャーク']\n",
    "    \n",
    "    # 類似条件の統計\n",
    "    similar_conditions = conditions[similar_indices]\n",
    "    condition_means = np.mean(similar_conditions, axis=0)\n",
    "    condition_stds = np.std(similar_conditions, axis=0)\n",
    "    \n",
    "    x_pos = np.arange(len(condition_names))\n",
    "    plt.bar(x_pos - 0.2, condition_means, 0.4, yerr=condition_stds, \n",
    "           alpha=0.7, label='Similar Original', capsize=5)\n",
    "    plt.bar(x_pos + 0.2, condition_vector, 0.4, \n",
    "           alpha=0.7, label='Target Condition')\n",
    "    \n",
    "    plt.xticks(x_pos, condition_names, rotation=45)\n",
    "    plt.ylabel('値')\n",
    "    plt.title('条件パラメータの比較')\n",
    "    plt.legend()\n,
    "    plt.grid(True, alpha=0.3)\n",
    "    \n",
    "    # 多様性分析\n",
    "    plt.subplot(2, 3, 5)\n",
    "    \n",
    "    # 既存データの多様性\n",
    "    original_diversity = []\n",
    "    for i in range(len(similar_trajectories)):\n",
    "        for j in range(i+1, len(similar_trajectories)):\n",
    "            dist = np.mean(np.sqrt(np.sum((similar_trajectories[i] - similar_trajectories[j])**2, axis=1)))\n",
    "            original_diversity.append(dist)\n",
    "    \n",
    "    # 生成データの多様性\n",
    "    generated_diversity = []\n",
    "    for i in range(len(generated_trajectories)):\n",
    "        for j in range(i+1, len(generated_trajectories)):\n",
    "            dist = np.mean(np.sqrt(np.sum((generated_trajectories[i] - generated_trajectories[j])**2, axis=1)))\n",
    "            generated_diversity.append(dist)\n",
    "    \n",
    "    if original_diversity and generated_diversity:\n",
    "        plt.boxplot([original_diversity, generated_diversity], \n",
    "                   labels=['Original', 'Generated'])\n",
    "        plt.ylabel('軌道間距離')\n",
    "        plt.title('データ多様性の比較')\n",
    "        plt.grid(True, alpha=0.3)\n",
    "    \n",
    "    # 数値統計\n",
    "    plt.subplot(2, 3, 6)\n",
    "    stats_text = f\"\"\"\n",
    "比較統計:\n",
    "    \n",
    "類似既存データ数: {len(similar_indices)}\n",
    "生成サンプル数: {len(generated_trajectories)}\n",
    "    \n",
    "軌道長:\n",
    "  既存: {np.mean(original_lengths):.3f}±{np.std(original_lengths):.3f}\n",
    "  生成: {np.mean(generated_lengths):.3f}±{np.std(generated_lengths):.3f}\n",
    "    \n",
    "多様性:\n",
    "  既存: {np.mean(original_diversity):.3f}±{np.std(original_diversity):.3f}\n",
    "  生成: {np.mean(generated_diversity):.3f}±{np.std(generated_diversity):.3f}\n",
    "    \"\"\"\n",
    "    \n",
    "    plt.text(0.1, 0.5, stats_text, fontsize=10, verticalalignment='center')\n",
    "    plt.axis('off')\n",
    "    plt.title('統計サマリー')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# 比較機能のボタンを追加\n",
    "if 'generator' in locals() and generator.current_trajectories is not None:\n",
    "    compare_button = widgets.Button(\n",
    "        description='既存データと比較',\n",
    "        button_style='success',\n",
    "        layout=widgets.Layout(width='200px')\n",
    "    )\n",
    "    \n",
    "    def on_compare_click(button):\n",
    "        condition_vector = np.array([\n",
    "            generator.movement_time_slider.value,\n",
    "            generator.endpoint_error_slider.value,\n",
    "            generator.jerk_slider.value\n",
    "        ])\n",
    "        compare_with_original(generator.current_trajectories, condition_vector, trajectories, conditions)\n",
    "    \n",
    "    compare_button.on_click(on_compare_click)\n",
    "    display(compare_button)\n",
    "    \n",
    "    print(\"軌道を生成してから '既存データと比較' ボタンを押してください。\")"
   ]
  }
 ],
