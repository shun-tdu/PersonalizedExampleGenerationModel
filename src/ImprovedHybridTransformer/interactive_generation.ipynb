   "source": [
    "# CLAUDE_ADDED\n",
    "# インタラクティブ生成UI\n",
    "class InteractiveGenerator:\n",
    "    def __init__(self, model, condition_stats, device):\n",
    "        self.model = model\n",
    "        self.condition_stats = condition_stats\n",
    "        self.device = device\n",
    "        self.current_trajectories = None\n",
    "        \n",
    "        # ウィジェット作成\n",
    "        self.create_widgets()\n",
    "        \n",
    "    def create_widgets(self):\n",
    "        # 条件パラメータのスライダー\n",
    "        self.movement_time_slider = FloatSlider(\n",
    "            value=self.condition_stats['mean'][0],\n",
    "            min=self.condition_stats['min'][0],\n",
    "            max=self.condition_stats['max'][0],\n",
    "            step=0.1,\n",
    "            description='動作時間:',\n",
    "            style={'description_width': 'initial'}\n",
    "        )\n",
    "        \n",
    "        self.endpoint_error_slider = FloatSlider(\n",
    "            value=self.condition_stats['mean'][1],\n",
    "            min=self.condition_stats['min'][1],\n",
    "            max=self.condition_stats['max'][1],\n",
    "            step=0.1,\n",
    "            description='終点誤差:',\n",
    "            style={'description_width': 'initial'}\n",
    "        )\n",
    "        \n",
    "        self.jerk_slider = FloatSlider(\n",
    "            value=self.condition_stats['mean'][2],\n",
    "            min=self.condition_stats['min'][2],\n",
    "            max=self.condition_stats['max'][2],\n",
    "            step=0.5,\n",
    "            description='ジャーク:',\n",
    "            style={'description_width': 'initial'}\n",
    "        )\n",
    "        \n",
    "        # サンプル数スライダー\n",
    "        self.num_samples_slider = IntSlider(\n",
    "            value=3,\n",
    "            min=1,\n",
    "            max=10,\n",
    "            description='サンプル数:',\n",
    "            style={'description_width': 'initial'}\n",
    "        )\n",
    "        \n",
    "        # 生成ボタン\n",
    "        self.generate_button = Button(\n",
    "            description='軌道生成',\n",
    "            button_style='info',\n",
    "            layout=widgets.Layout(width='150px')\n",
    "        )\n",
    "        self.generate_button.on_click(self.generate_trajectories)\n",
    "        \n",
    "        # ランダム条件ボタン\n",
    "        self.random_button = Button(\n",
    "            description='ランダム条件',\n",
    "            button_style='warning',\n",
    "            layout=widgets.Layout(width='150px')\n",
    "        )\n",
    "        self.random_button.on_click(self.set_random_conditions)\n",
    "        \n",
    "        # 出力エリア\n",
    "        self.output = Output()\n",
    "        \n",
    "    def set_random_conditions(self, button):\n",
    "        \"\"\"ランダムな条件を設定\"\"\"\n",
    "        # データセットからランダムに選択\n",
    "        random_idx = np.random.randint(0, len(conditions))\n",
    "        random_condition = conditions[random_idx]\n",
    "        \n",
    "        self.movement_time_slider.value = float(random_condition[0])\n",
    "        self.endpoint_error_slider.value = float(random_condition[1])\n",
    "        self.jerk_slider.value = float(random_condition[2])\n",
    "        \n",
    "        print(f\"ランダム条件を設定: 動作時間={random_condition[0]:.3f}, 終点誤差={random_condition[1]:.3f}, ジャーク={random_condition[2]:.3f}\")\n",
    "    \n",
    "    def generate_trajectories(self, button):\n",
    "        \"\"\"軌道を生成して可視化\"\"\"\n",
    "        with self.output:\n",
    "            clear_output(wait=True)\n",
    "            \n",
    "            # 条件を準備\n",
    "            condition = torch.tensor([\n",
    "                [self.movement_time_slider.value,\n",
    "                 self.endpoint_error_slider.value,\n",
    "                 self.jerk_slider.value]\n",
    "            ], dtype=torch.float32).to(self.device)\n",
    "            \n",
    "            print(f\"生成中... 条件: 動作時間={condition[0,0]:.3f}, 終点誤差={condition[0,1]:.3f}, ジャーク={condition[0,2]:.3f}\")\n",
    "            \n",
    "            # 軌道生成\n",
    "            with torch.no_grad():\n",
    "                generated = self.model.generate(\n",
    "                    condition=condition,\n",
    "                    sequence_length=trajectories.shape[1],\n",
    "                    num_samples=self.num_samples_slider.value\n",
    "                )\n",
    "            \n",
    "            self.current_trajectories = generated.cpu().numpy()\n",
    "            \n",
    "            # 可視化\n",
    "            self.visualize_results()\n",
    "    \n",
    "    def visualize_results(self):\n",
    "        \"\"\"結果を可視化\"\"\"\n",
    "        if self.current_trajectories is None:\n",
    "            return\n",
    "        \n",
    "        fig, axes = plt.subplots(1, 3, figsize=(18, 5))\n",
    "        \n",
    "        # 軌道プロット\n",
    "        ax1 = axes[0]\n",
    "        colors = ['red', 'blue', 'green', 'orange', 'purple', 'brown', 'pink', 'gray', 'olive', 'cyan']\n",
    "        \n",
    "        for i, traj in enumerate(self.current_trajectories):\n",
    "            color = colors[i % len(colors)]\n",
    "            ax1.plot(traj[:, 0], traj[:, 1], '-', color=color, linewidth=2, alpha=0.8, label=f'Sample {i+1}')\n",
    "            ax1.scatter(traj[0, 0], traj[0, 1], color=color, s=100, marker='o', alpha=0.8)  # 開始点\n",
    "            ax1.scatter(traj[-1, 0], traj[-1, 1], color=color, s=100, marker='s', alpha=0.8)  # 終点\n",
    "        \n",
    "        ax1.set_xlabel('X Position')\n",
    "        ax1.set_ylabel('Y Position')\n",
    "        ax1.set_title('生成された軌道')\n",
    "        ax1.legend(bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "        ax1.grid(True, alpha=0.3)\n",
    "        ax1.axis('equal')\n",
    "        \n",
    "        # X軸時系列\n",
    "        ax2 = axes[1]\n",
    "        for i, traj in enumerate(self.current_trajectories):\n",
    "            color = colors[i % len(colors)]\n",
    "            ax2.plot(traj[:, 0], color=color, linewidth=2, alpha=0.8, label=f'Sample {i+1}')\n",
    "        \n",
    "        ax2.set_xlabel('Time Step')\n",
    "        ax2.set_ylabel('X Position')\n",
    "        ax2.set_title('X軸時系列')\n",
    "        ax2.grid(True, alpha=0.3)\n",
    "        \n",
    "        # Y軸時系列\n",
    "        ax3 = axes[2]\n",
    "        for i, traj in enumerate(self.current_trajectories):\n",
    "            color = colors[i % len(colors)]\n",
    "            ax3.plot(traj[:, 1], color=color, linewidth=2, alpha=0.8, label=f'Sample {i+1}')\n",
    "        \n",
    "        ax3.set_xlabel('Time Step')\n",
    "        ax3.set_ylabel('Y Position')\n",
    "        ax3.set_title('Y軸時系列')\n,
    "        ax3.grid(True, alpha=0.3)\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # 統計情報表示\n",
    "        print(f\"\\n生成統計:\")\n",
    "        print(f\"  サンプル数: {len(self.current_trajectories)}\")\n",
    "        print(f\"  系列長: {self.current_trajectories.shape[1]}\")\n",
    "        \n",
    "        # 軌道長の計算\n",
    "        trajectory_lengths = []\n",
    "        for traj in self.current_trajectories:\n",
    "            length = np.sum(np.sqrt(np.sum(np.diff(traj, axis=0)**2, axis=1)))\n",
    "            trajectory_lengths.append(length)\n",
    "        \n",
    "        print(f\"  軌道長: 平均={np.mean(trajectory_lengths):.3f}, 標準偏差={np.std(trajectory_lengths):.3f}\")\n",
    "        \n",
    "        # 終点位置\n",
    "        end_positions = self.current_trajectories[:, -1, :]\n",
    "        print(f\"  終点X: 平均={np.mean(end_positions[:, 0]):.3f}, 標準偏差={np.std(end_positions[:, 0]):.3f}\")\n",
    "        print(f\"  終点Y: 平均={np.mean(end_positions[:, 1]):.3f}, 標準偏差={np.std(end_positions[:, 1]):.3f}\")\n",
    "    \n",
    "    def display(self):\n",
    "        \"\"\"ウィジェットを表示\"\"\"\n",
    "        control_box = widgets.VBox([\n",
    "            widgets.HTML(\"<h3>軌道生成パラメータ</h3>\"),\n",
    "            self.movement_time_slider,\n",
    "            self.endpoint_error_slider,\n",
    "            self.jerk_slider,\n",
    "            self.num_samples_slider,\n",
    "            widgets.HBox([self.generate_button, self.random_button])\n",
    "        ])\n",
    "        \n",
    "        display(control_box)\n",
    "        display(self.output)\n",
    "\n",
    "# ジェネレーターを作成\n",
    "if 'model' in locals():\n",
    "    generator = InteractiveGenerator(model, condition_stats, device)\n",
    "    generator.display()\nelse:\n",
    "    print(\"モデルが読み込まれていません。上のセルを実行してください。\")"
   ]
  },
