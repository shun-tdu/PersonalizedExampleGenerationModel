   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# CLAUDE_ADDED\n",
    "# モデルとデータの読み込み\n",
    "model_path = 'outputs/hybrid_model_final.pth'  # 学習済みモデルのパス\n",
    "data_path = '../../data/Datasets/overfitting_dataset.npz'\n",
    "\n",
    "# デバイス設定\n",
    "device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
    "print(f\"使用デバイス: {device}\")\n",
    "\n",
    "# データ読み込み\n",
    "trajectories, conditions = load_processed_data(data_path)\n",
    "print(f\"データ形状: 軌道={trajectories.shape}, 条件={conditions.shape}\")\n",
    "\n",
    "# 条件の統計情報\n",
    "condition_names = ['動作時間', '終点誤差', 'ジャーク']\n",
    "condition_stats = {\n",
    "    'min': conditions.min(axis=0),\n",
    "    'max': conditions.max(axis=0),\n",
    "    'mean': conditions.mean(axis=0),\n",
    "    'std': conditions.std(axis=0)\n",
    "}\n",
    "\n",
    "for i, name in enumerate(condition_names):\n",
    "    print(f\"{name}: 範囲[{condition_stats['min'][i]:.3f}, {condition_stats['max'][i]:.3f}], 平均={condition_stats['mean'][i]:.3f}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# CLAUDE_ADDED\n",
    "# モデル読み込み\n",
    "if os.path.exists(model_path):\n",
    "    model = HybridTrajectoryModel.load_model(model_path, device=device)\n",
    "    model.eval()\n",
    "    print(\"モデル読み込み成功!\")\n",
    "    \n",
    "    # モデル情報表示\n",
    "    model_info = model.get_model_info()\n",
    "    print(f\"総パラメータ数: {model_info['total_parameters']:,}\")\n",
    "    print(f\"低周波モデル: {model_info['low_freq_parameters']:,}\")\n",
    "    print(f\"高周波モデル: {model_info['high_freq_parameters']:,}\")\nelse:\n",
    "    print(f\"エラー: モデルファイルが見つかりません: {model_path}\")\n",
    "    print(\"先に学習を実行してください\")"
   ]
